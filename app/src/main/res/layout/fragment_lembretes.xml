<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentAvaliacoes">

    <ImageView
        android:id="@+id/ConcluidoLembrete"
        android:layout_width="167dp"
        android:layout_height="175dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.307"
        app:srcCompat="@drawable/tudo_concluido" />

    <TextView
        android:id="@+id/semLembretes"
        style="@style/nunito_regular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-20dp"
        android:text="Sem lembretes"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="@+id/ConcluidoLembrete"
        app:layout_constraintStart_toStartOf="@+id/ConcluidoLembrete"
        app:layout_constraintTop_toBottomOf="@+id/ConcluidoLembrete" />

</androidx.constraintlayout.widget.ConstraintLayout>